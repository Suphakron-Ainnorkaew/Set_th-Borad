<div class="d-flex justify-content-between flex-wrap flex-md nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
    <h1 class="h2">Editing: <%= setmodel.company %> <%= setmodel.group %></h1>
    <div class="btn-toolbar mb-2 mb-md 0">
      <div class="btn-group me-2">
        <button class="btn btn-sm btn-outline-secondary">?</button>
      </div>
    </div>
  </div>
  
  <div class="col py-3">
    <div class="row">
      <div class="col">
        <nav aria-label="breadcrumb">
          <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/">Dashboard</a></li>
            <li class="breadcrumb-item active"><%= setmodel.company %> <%= setmodel.group %></li>
          </ol>
        </nav>
      </div>
      <div class="col text-end fw-lighter">
        <b>Last Updated: <%= new Date(setmodel.updatedAt).toUTCString()  %></b>
        <b>Setid:</b> <%= setmodel._id %>
      </div>
    </div>
  
  </div>
  
  
  <form action="/edit/<%= setmodel._id %>?_method=PUT" method="POST">
  
    <div class="row form-group mb-4">
      <div class="col">
        <label for="firstName">company</label>
        <input type="text" class="form-control" id="company" name="company" value="<%= setmodel.company %>" placeholder="company" required>
      </div>
  
      <div class="col">
        <label for="lastName">setname</label>
        <input type="text" class="form-control" id="setname" name="setname" value="<%= setmodel.setname %>" placeholder="setname" required>
      </div>
    </div>
  
    <div class="row form-group mb-4">
      <div class="col">
        <label for="tel">group</label>
        <input type="text" class="form-control" id="group" name="group" value="<%= setmodel.group %>" placeholder="group" required>
      </div>
  
    <div class="col">
        <label for="email">Priceaverage</label>
        <input type="text" class="form-control" id="Priceaverage" name="Priceaverage" value="<%= setmodel.Priceaverage %>" placeholder="Priceaverage" required>
      </div>
    </div>

    <div class="col">
        <label for="email">Profitaverage</label>
        <input type="text" class="form-control" id="Profitaverage" name="Profitaverage" value="<%= setmodel.Profitaverage %>" placeholder="Profitaverage" required>
      </div>
    </div>

    <div class="col">
        <label for="email">Averagedividend</label>
        <input type="text" class="form-control" id="Averagedividend" name="Averagedividend" value="<%= setmodel.Averagedividend %>" placeholder="Averagedividend" required>
      </div>
    </div>

    <div class="col">
        <label for="email">margin</label>
        <input type="text" class="form-control" id="margin" name="margin" value="<%= setmodel.margin %>" placeholder="margin" required>
      </div>
    </div>
  
    <div class="form-group mb-4">
      <button type="submit" class="btn btn-primary">Update SEt</button>
  
      <button type="button" class="btn btn-danger" data-bs-toggle="modal" id="deleteButton" data-bs-target="#deleteModal">Delete Set</button>
  
    </div>
  
  </form>
  
  
  
  <div class="modal fade" tabindex="-1" role="dialog" id="deleteModal">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <div class="modal-title">You are about to remove a set record.</div>
          <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
        </div>
        <div class="modal-body">
          <p>
            This will remove the set record of <b class="fw-bold"><%= setmodel.company %> <%= setmodel.Ld %></b><br/>
            Are you sure?
          </p>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
          <form action="/edit/<%= setmodel._id %>?_method=DELETE" method="POST" class="position-relative">
            <button type="submit" class="btn btn-primary">Yes, Remove Customer</button>
          </form>
        </div>
      </div>
    </div>
  </div>